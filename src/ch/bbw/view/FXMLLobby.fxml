<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="227.0" prefWidth="528.0"
            style="-fx-background-color: #2c3e50;" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ch.bbw.controller.FXMLLobbyController">
    <children>
        <Label fx:id="username" contentDisplay="CENTER" layoutX="462.0" layoutY="24.0" prefHeight="17.0"
               prefWidth="66.0" textAlignment="CENTER" textFill="#2c3e50"/>
        <Label layoutX="14.0" layoutY="7.0" prefHeight="50.0" prefWidth="378.0" text="Game of Life &amp; Death">
            <font>
                <Font name="OCR A Extended" size="30.0"/>
            </font>
            <textFill>
                <LinearGradient endX="1.0" endY="0.6259541839133692" startX="1.0" startY="1.0">
                    <stops>
                        <Stop color="#000000ab"/>
                        <Stop color="#000000ab" offset="0.007633587786259542"/>
                        <Stop color="WHITE" offset="1.0"/>
                    </stops>
                </LinearGradient>
            </textFill>
        </Label>
        <HBox layoutX="17.0" layoutY="57.0" prefHeight="179.0" prefWidth="500.0">
            <children>
                <VBox prefHeight="179.0" prefWidth="240.0">
                    <children>
                        <Label text="Received Invites" textFill="WHITE">
                            <font>
                                <Font name="System Bold" size="12.0"/>
                            </font>
                        </Label>
                        <VBox fx:id="receivedInvites" prefHeight="153.0" prefWidth="240.0"
                              style="-fx-border-color: #1F2D3F;"/>
                    </children>
                </VBox>
                <VBox prefHeight="179.0" prefWidth="250.0">
                    <children>
                        <Label text="Send Invites" textFill="WHITE">
                            <font>
                                <Font name="System Bold" size="12.0"/>
                            </font>
                        </Label>
                        <VBox fx:id="users" prefHeight="203.0" prefWidth="250.0" style="-fx-border-color: #1F2D3F;"/>
                        <HBox prefHeight="28.0" prefWidth="286.0">
                            <VBox.margin>
                                <Insets top="10.0"/>
                            </VBox.margin>
                        </HBox>
                    </children>
                    <HBox.margin>
                        <Insets left="10.0"/>
                    </HBox.margin>
                </VBox>
            </children>
        </HBox>
        <ImageView fitHeight="67.0" fitWidth="101.0" layoutX="412.0" layoutY="8.0" pickOnBounds="true"
                   preserveRatio="true"/>
    </children>
</AnchorPane>
